    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "esbuild": "^0.24.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1"
