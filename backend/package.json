  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsup",
    "build:only": "tsc",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "lint": "eslint . --max-warnings 0",
